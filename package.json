{
  "name": "covulpecker",
  "displayName": "CoVulPecker",
  "description": "A plugin for vulnerability detection in C/C++ source code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "covulpecker-sidebar-view",
          "title": "Extension TeamQ",
          "icon": "media/svgs/malware.svg"
        }
      ]
    },
    "views": {
      "covulpecker-sidebar-view": [
        {
          "type": "webview",
          "id": "covulpecker.sidebar",
          "name": "Group2",
          "icon": "media/svgs/malware.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "covulpecker.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "type": "commonjs",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "rollup -c && webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
    "pretest": "npm run package && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tsconfig/svelte": "^5.0.4",
    "@types/btoa": "^1.2.5",
    "@types/glob": "^8.1.0",
    "@types/googlemaps": "^3.43.3",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.11",
    "@types/polka": "^0.5.7",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "glob": "^10.4.5",
    "mocha": "^10.7.3",
    "rollup": "^4.21.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.2.2",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.6",
    "svelte-preprocess": "^6.0.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "vscode-test": "^1.6.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/vscode-webview": "^1.57.5",
    "btoa": "^1.2.1",
    "c-ast": "^1.0.0",
    "covulpecker": "file:",
    "date-fns": "^3.6.0",
    "node-fetch": "^3.3.2",
    "polka": "^0.5.2",
    "reconnecting-websocket": "^4.4.0",
    "rollup-plugin-css-only": "^4.5.2"
  }
}
